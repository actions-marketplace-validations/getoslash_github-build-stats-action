{
  "name": "github-build-stats-action",
  "version": "1.0.0",
  "description": "GitHub Action workflow run trends",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/getoslash/github-build-stats-action"
  },
  "scripts": {
    "build": "tsc && ncc build lib/main.js --license licenses.txt && npx rimraf lib/",
    "test": "tape -r ts-node/register/transpile-only tests/**/*.test.ts",
    "coverage": "nyc npm run test",
    "lint": "tsc --noEmit && eslint --cache \"**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "preprepare": "husky install",
    "prepare": "npm run build",
    "release": "semantic-release"
  },
  "dependencies": {
    "@actions/core": "1.4.0",
    "@actions/github": "5.0.0",
    "@octokit/rest": "18.8.0",
    "dayjs": "1.10.6",
    "percentile": "1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@types/node": "16.4.10",
    "@types/tape": "4.13.2",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "@vercel/ncc": "0.29.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "git-notify": "0.2.3",
    "hook-std": "2.0.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.1",
    "nock": "13.1.1",
    "nyc": "15.1.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.4",
    "tape": "5.3.0",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  }
}
